# Convert the certificate and private key into a PKCS12 file
openssl pkcs12 -export -in yourdomain.crt -inkey yourdomain.key -out yourdomain.p12 -name ujjivan_2024 -password iibprod@123

# Convert PKCS12 to JKS
keytool -importkeystore -srckeystore yourdomain.p12 -srcstoretype PKCS12 -destkeystore yourdomain.jks -deststoretype JKS -srcstorepass iibprod@123 -deststorepass iibprod@123



keytool -import -trustcacerts -alias ujjivan_2024 -file yourdomain.crt -keystore yourdomain.jks -storepass iibprod@123


mqsichangeproperties BABRKP01 -o BrokerRegistry  –n  brokerKeystoreFile -v /home/mqm/yourdomain.jks
mqsichangeproperties BABRKP01 -o BrokerRegistry   -n  brokerKeystoreFile  -v /home/mqm/yourdomain.jks


mqsichangeproperties broker_name -o BrokerRegistry    –n  brokerTruststoreFile   -v c:\truststore\server.truststore


mqsisetdbparms BABRKP01   -n brokerKeystore::password   -u temp -p iibprod@123
mqsisetdbparms BABRKP01   -n brokerTruststore::password   -u temp -p iibprod@123




