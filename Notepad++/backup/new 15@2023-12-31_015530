		DECLARE DSN1 CHARACTER 'scott';
		DECLARE ESB_AUDIT_LOG ROW;
		CREATE LASTCHILD OF Environment DOMAIN 'JSON' NAME 'JSON';
		FOR OutRef AS Environment.ESB_AUDIT_MESSAGE.Item[] DO
			
			SET Environment.JSON.Data = OutRef;
			SET ESB_AUDIT_LOG.AID = OutRef.AID;
			SET ESB_AUDIT_LOG.SERVICE_NAME = OutRef.SERVICE_NAME;
			SET ESB_AUDIT_LOG.UNIQUE_MSG_ID = OutRef.UNIQUE_MSG_ID;
			SET ESB_AUDIT_LOG.PROVIDER_ID = OutRef.PROVIDER_ID;
			SET ESB_AUDIT_LOG.PAYLOAD = CAST(ASBITSTREAM(Environment.JSON.Data) AS CHARACTER CCSID InputRoot.Properties.CodedCharSetId Encoding InputRoot.Properties.Encoding);
			SET ESB_AUDIT_LOG.TIMESTAMP= CURRENT_TIMESTAMP;
			SET ESB_AUDIT_LOG.LOG_POINT = OutRef.LOG_POINT;
			SET ESB_AUDIT_LOG.STATUS = OutRef.STATUS;
			SET ESB_AUDIT_LOG.URL = OutRef.URL;
			SET ESB_AUDIT_LOG.ERROR_CODE = OutRef.ERROR_CODE;
			SET ESB_AUDIT_LOG.ERROR_DESCRIPTION = OutRef.ERROR_DESCRIPTION;
			SET ESB_AUDIT_LOG.SERVER = OutRef.SERVER;
			SET ESB_AUDIT_LOG.HOST = OutRef.HOST;
			SET ESB_AUDIT_LOG.PORT = OutRef.PORT;

			PASSTHRU INSERTPASSTHRU TO Database.{DSN1} VALUES (ESB_AUDIT_LOG.[]) ;
		END FOR;